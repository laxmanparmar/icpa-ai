{
  "name": "upload-service",
  "version": "1.0.0",
  "description": "Upload service with presigned S3 URLs, Express locally, Lambda for production",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "local": "ts-node-dev --respawn --transpile-only src/server.ts",
    "deploy": "serverless deploy",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "express",
    "aws",
    "s3",
    "lambda",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "serverless-http": "^3.2.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "serverless": "^3.38.0",
    "serverless-plugin-typescript": "^2.1.5"
  }
}

